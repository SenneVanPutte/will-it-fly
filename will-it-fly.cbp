<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="will-it-fly" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="all">
				<Option type="4" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="demos">
				<Option type="4" />
				<Option compiler="gcc" />
			</Target>
			<Target title="wif">
				<Option output="will-it-fly" prefix_auto="1" extension_auto="1" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
			</Target>
			<Target title="wif_core">
				<Option output="will-it-fly" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
			</Target>
			<Target title="wif_viz">
				<Option output="will-it-fly" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
			</Target>
			<Target title="wif_algo">
				<Option output="will-it-fly" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
			</Target>
			<Target title="clean">
				<Option type="4" />
				<Option compiler="gcc" />
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="wif-algo/wif_algo.cpp">
			<Option target="wif" />
			<Option target="wif_algo" />
		</Unit>
		<Unit filename="wif-algo/wif_algo.hpp">
			<Option target="wif" />
			<Option target="wif_algo" />
		</Unit>
		<Unit filename="wif-core/wif_core.cpp">
			<Option target="wif" />
			<Option target="wif_core" />
		</Unit>
		<Unit filename="wif-core/wif_core.hpp">
			<Option target="wif" />
			<Option target="wif_core" />
		</Unit>
		<Unit filename="wif-viz/wif_viz.cpp">
			<Option target="wif" />
			<Option target="wif_viz" />
		</Unit>
		<Unit filename="wif-viz/wif_viz.hpp">
			<Option target="wif" />
			<Option target="wif_viz" />
		</Unit>
		<Unit filename="will-it-fly/main.cpp">
			<Option target="wif" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
